delete mesh
sculpt parallel volume all size 0.75

#create symmetric model
delete mesh
vol 1 rotate 45 about z 
sculpt parallel volume all size 3

#example 4
reset
import acis "bolt-plate.sat" 
#mesh with size of 0.2
expect error 1 less 
sculpt parallel volume all size 0.2 curve_opt_thresh -1.0
expect error stop
expect error 1
quality hex all scaled jacobian draw mesh
expect error stop

#mesh with 'auto pillowing' and 'local at curves' options, lowest quality is 0.271
del mesh
sculpt parallel volume all size 0.2 pillow 212
quality hex all scaled jacobian draw mesh
#mesh with 'auto pillowing' and 'all surfaces' option, lowest quality is 0.305
del mesh
sculpt parallel volume all size 0.2 pillow 1
quality hex all scaled jacobian draw mesh

